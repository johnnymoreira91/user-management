{
  "name": "user-management",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Johnny Moreira",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "prod": "NODE_ENV=prod pm2 start dist/index.js --max-memory-restart 512M --name Sales_Back",
    "stop": "pm2 stop all && pm2 delete all",
    "start": "NODE_ENV=prod node dist/index.js",
    "dev": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "test": "NODE_ENV=test jest --forceExit"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "kafkajs": "^2.2.0",
    "knex": "^2.3.0",
    "lib-bets": "^1.0.5",
    "morgan": "^1.10.0",
    "objection": "^3.0.1",
    "pm2": "^5.1.2",
    "socket.io": "^4.5.2",
    "sqlite3": "^5.1.1",
    "uuid": "^9.0.0",
    "winston": "^3.5.1"
  }
}
